{
  "verbose": false,
  "stream": false,

  "input": {
    "puller": {
      "model": "HWPCReport",
      "type": "mongodb",
      "uri": "mongodb://localhost:27017",
      "db": "PowerAPI_db",
      "collection": "sensor_values"
    }
  },

  "output": {
    "pusher_power": {
      "model": "HWPCReport",
      "type": "mongodb",
      "uri": "mongodb://localhost:27017",
      "db": "PowerAPI_db",
      "collection": "power_values"
    }
  },

  "cpu-base-freq": 1900,
  "cpu-error-threshold": 2.0,
  "disable-dram-formula": true,
  "sensor-reports-frequency": 1000
}